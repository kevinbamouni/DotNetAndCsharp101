<Window x:Class="WpfChinookDataApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfChinookDataApp"
        mc:Ignorable="d"
        Title="Title" Height="450" Width="800">
    <Window.Resources>
        <local:MainWindow x:Key="LocalMainWindow"/>
    </Window.Resources>
    <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*" />
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="*" />
                <RowDefinition Height="*" />
            </Grid.RowDefinitions>
        <DataGrid x:Name="dataGrid" Grid.Row="1" d:Visibility="Visible" d:IsReadOnly="True"/>
    </Grid>
</Window>
